<?xml version="1.0"?>
<doc>
  <basic type="animations">
    <screenCanvasResolution>
      <width>100</width>
      <height>100</height>
    </screenCanvasResolution>
    <canvasResolution>
      <maxWidth>100</maxWidth>
      <maxHeight>100</maxHeight>
    </canvasResolution>
    <bgColor>#fffafafa</bgColor>
  </basic>
  <images>
    <image>
      <fileName>cube.png</fileName>
      <imagePropertiesAnimation>
        <propetiesToAnimate>
          <property>
            <propertyName>Rotation</propertyName>
            <minimumDegreesValue>0</minimumDegreesValue>
            <maximumDegreesValue>360</maximumDegreesValue>
            <updateDegreesValue>6</updateDegreesValue>
            <currentDegreesValue>0</currentDegreesValue>
            <rotationSpeed>1</rotationSpeed>
            <animationRotationAction>Increase</animationRotationAction>
          </property>
        </propetiesToAnimate>
      </imagePropertiesAnimation>
      <left>25</left>
      <top>25</top>
      <speed>2</speed>
    </image>
  </images>
</doc>
