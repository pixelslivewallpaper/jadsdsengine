<?xml version="1.0"?>
<doc>
  <basic type="animations">
    <screenCanvasResolution>
      <width>160</width>
      <height>240</height>
    </screenCanvasResolution>
    <canvasResolution>
      <maxWidth>220</maxWidth>
      <maxHeight>240</maxHeight>
    </canvasResolution>
    <bgColor>#ff000000</bgColor>
  </basic>
  <timers>
    <timer>
      <timerDelay>5000</timerDelay>
      <timerStop>false</timerStop>
      <id>timer1</id>
      <events>
        <event>
          <eventActions>
            <eventAction>ChangeCustomCondition</eventAction>
          </eventActions>
          <editAnimationByIDOptions>
            <option>
              <id>frame1</id>
              <editActions>
                <editAction>UpdateImagePropertiesAnimation</editAction>
                <editAction>StopAnimation</editAction>
              </editActions>
              <newImagePropertiesAnimation>
                <propetiesToAnimate>
                  <property>
                    <propertyName>Rotation</propertyName>

                    <updateDegreesValue>1</updateDegreesValue>
                    <animationRotationAction>Increase</animationRotationAction>
                  </property>
                </propetiesToAnimate>
              </newImagePropertiesAnimation>
            </option>
          </editAnimationByIDOptions>
          <whenCustomConditionIs>aniA</whenCustomConditionIs>
          <newCustomCondition>aniB</newCustomCondition>
        </event>
        <event>
          <eventActions>
            <eventAction>ChangeCustomCondition</eventAction>
          </eventActions>
          <editAnimationByIDOptions>
            <option>
              <id>frame1</id>
              <editActions>
                <editAction>UpdateImagePropertiesAnimation</editAction>
                <editAction>ChangeFrameCollection</editAction>
                <editAction>ResumeAnimation</editAction>
              </editActions>
              <changeFrameCollectionIndexTo>0</changeFrameCollectionIndexTo>
              <newImagePropertiesAnimation>
                <propetiesToAnimate>
                  <property>
                    <propertyName>Rotation</propertyName>
                    <updateDegreesValue>3</updateDegreesValue>
                    <animationRotationAction>Increase</animationRotationAction>
                  </property>
                </propetiesToAnimate>
              </newImagePropertiesAnimation>
            </option>
          </editAnimationByIDOptions>
          <whenCustomConditionIs>aniB</whenCustomConditionIs>
          <newCustomCondition>aniC</newCustomCondition>
        </event>
        <event>
          <eventActions>
            <eventAction>ChangeCustomCondition</eventAction>
          </eventActions>
          <editAnimationByIDOptions>
            <option>
              <id>frame1</id>
              <editActions>
                <editAction>UpdateImagePropertiesAnimation</editAction>
                <editAction>StopAnimation</editAction>
              </editActions>
              <newImagePropertiesAnimation>
                <propetiesToAnimate>
                  <property>
                    <propertyName>Rotation</propertyName>
                    <updateDegreesValue>1</updateDegreesValue>
                    <animationRotationAction>Increase</animationRotationAction>
                  </property>
                </propetiesToAnimate>
              </newImagePropertiesAnimation>
            </option>
          </editAnimationByIDOptions>
          <whenCustomConditionIs>aniC</whenCustomConditionIs>
          <newCustomCondition>aniD</newCustomCondition>
        </event>
        <event>
          <eventActions>
            <eventAction>ChangeCustomCondition</eventAction>
          </eventActions>
          <editAnimationByIDOptions>
            <option>
              <id>frame1</id>
              <editActions>
                <editAction>UpdateImagePropertiesAnimation</editAction>
                <editAction>ChangeFrameCollection</editAction>
                <editAction>ResumeAnimation</editAction>
              </editActions>
              <changeFrameCollectionIndexTo>1</changeFrameCollectionIndexTo>
              <newImagePropertiesAnimation>
                <propetiesToAnimate>
                  <property>
                    <propertyName>Rotation</propertyName>
                    <updateDegreesValue>2</updateDegreesValue>
                    <animationRotationAction>Decrease</animationRotationAction>
                  </property>
                </propetiesToAnimate>
              </newImagePropertiesAnimation>
            </option>
          </editAnimationByIDOptions>
          <whenCustomConditionIs>aniD</whenCustomConditionIs>
          <newCustomCondition>aniA</newCustomCondition>
        </event>
        <defaultCustomCondition>aniA</defaultCustomCondition>
      </events>
    </timer>
  </timers>
  <images>
    <image>
      <left>-60</left>
      <top>-50</top>
      <id>back</id>
      <fileName>back.png</fileName>
      <imagePropertiesAnimation>
        <propetiesToAnimate>
          <property>
            <propertyName>Rotation</propertyName>
            <minimumDegreesValue>0</minimumDegreesValue>
            <maximumDegreesValue>360</maximumDegreesValue>
            <updateDegreesValue>1</updateDegreesValue>
            <currentDegreesValue>0</currentDegreesValue>
            <rotationSpeed>1</rotationSpeed>
            <animationRotationAction>Increase</animationRotationAction>
          </property>
        </propetiesToAnimate>
      </imagePropertiesAnimation>
    </image>
    <image>
      <frames>
        <frameCollection>
          <frame>
            <fileName>frame1.png</fileName>
          </frame>
          <frame>
            <fileName>frame2.png</fileName>
          </frame>
          <frame>
            <fileName>frame3.png</fileName>
          </frame>
          <frame>
            <fileName>frame2.png</fileName>
          </frame>
        </frameCollection>
        <frameCollection>
          <frame>
            <fileName>frame1.png</fileName>
          </frame>
          <frame>
            <fileName>frame2.png</fileName>
          </frame>
          <frame>
            <fileName>frame3.png</fileName>
          </frame>
          <frame>
            <fileName>frame2.png</fileName>
          </frame>
          <applyAlterationsToAllFrames>
            <alterationsToDo>FlipHorizontal</alterationsToDo>
          </applyAlterationsToAllFrames>
        </frameCollection>
        <framesAnimationSpeed>2</framesAnimationSpeed>
        <defaultFrameCollection>1</defaultFrameCollection>
      </frames>
      <left>18</left>
      <top>28</top>
      <id>frame1</id>
      <imagePropertiesAnimation>
        <propetiesToAnimate>
          <property>
            <propertyName>Rotation</propertyName>
            <minimumDegreesValue>0</minimumDegreesValue>
            <maximumDegreesValue>360</maximumDegreesValue>
            <updateDegreesValue>2</updateDegreesValue>
            <currentDegreesValue>0</currentDegreesValue>
            <rotationSpeed>1</rotationSpeed>
            <animationRotationAction>Decrease</animationRotationAction>
          </property>
        </propetiesToAnimate>
      </imagePropertiesAnimation>
    </image>
  </images>
</doc>
