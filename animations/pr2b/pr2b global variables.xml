<?xml version="1.0"?>
<doc>
  <basic type="animations">
    <screenCanvasResolution>
      <width>320</width>
      <height>480</height>
    </screenCanvasResolution>
    <canvasResolution>
      <maxWidth>144</maxWidth>
      <maxHeight>72</maxHeight>
    </canvasResolution>
    <bgColor>#ff000000</bgColor>
    <grid>
      <addGrid>true</addGrid>
      <gridColor>#ff000000</gridColor>
      <addPixelBetweenGrid>true</addPixelBetweenGrid>
    </grid>
    <camera>
      <align>Custom</align>
      <customAlignLeft>0</customAlignLeft>
      <!--<customAlignTop>100</customAlignTop>-->

      <!--<autoHorizontalScroll>true</autoHorizontalScroll>
      <iniPosition>0;0</iniPosition>
      <pixelsMove>2</pixelsMove>
      <cameraSpeed>1</cameraSpeed>-->
    </camera>    
  </basic>
  <globalVariables>
    <variable>
      <id>uno</id>
      <value>beta</value>
    </variable>
    <variable>
      <id>dos</id>
      <value>killme</value>
    </variable>
  </globalVariables>
  <images>
    <image>
      <fileName>pr2bb3.png</fileName>
      <grid>
        <addGrid>false</addGrid>
      </grid>
    </image>
    <image>
      <frames>
        <frame>
          <fileName>pr2bb4.png</fileName>
        </frame>
        <frame>
          <fileName>pr2bb5.png</fileName>
        </frame>
        <frame>
          <name></name>
        </frame>
      </frames>
      <grid>
        <addGrid>false</addGrid>
      </grid>
      <speed>5</speed>
    </image>
    <image>
      <fileName>pr2bb1.png</fileName>
      <left>0</left>
      <top>0</top>
      <events>
        <event>
          <eventType>OnClickUp</eventType>
          <eventActions>
            <eventAction>UpdateGlobalVariable</eventAction>
          </eventActions>
          <globalVariable>
            <id>uno</id>
            <value>hello</value>
          </globalVariable>
        </event>
      </events>
    </image>
    <image>
      <fileName>pr2bb2.png</fileName>
      <left>11</left>
      <top>21</top>
      <events>
        <event>
          <eventType>OnClickUp</eventType>
          <eventActions>
            <eventAction>ChangeVisibleValue</eventAction>
          </eventActions>
          <newVisibleValue>false</newVisibleValue>
          <whenGlobalVariableIs>
            <id>uno</id>
            <value>hello</value>
