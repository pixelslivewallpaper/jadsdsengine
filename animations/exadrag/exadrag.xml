<?xml version="1.0"?>
<doc>
  <basic type="animations">
    <screenCanvasResolution>
      <width>100</width>
      <height>100</height>
    </screenCanvasResolution>
    <canvasResolution>
      <maxWidth>100</maxWidth>
      <maxHeight>100</maxHeight>
    </canvasResolution>
    <bgColor>#fffafafa</bgColor>
  </basic>
  <images>
    <image>
      <frames>
        <frameCollection>
          <frame>
            <fileName>drag1.png</fileName>
          </frame>
        </frameCollection>
        <frameCollection>
          <frame>
            <fileName>drag2.png</fileName>
          </frame>
        </frameCollection>
      </frames>
      <left>20</left>
      <top>20</top>
      <events>
        <event>
          <eventType>DragAndDrop</eventType>
          <eventActions>
            <eventAction>DragAndDropMove</eventAction>
          </eventActions>
        </event>
        <event>
          <eventType>OnMouseOver</eventType>
          <eventActions>
            <eventAction>ChangeFrameCollection</eventAction>
          </eventActions>
          <changeFrameCollectionIndexTo>1</changeFrameCollectionIndexTo>
        </event>
        <event>
          <eventType>OnMouseOut</eventType>
          <eventActions>
            <eventAction>ChangeFrameCollection</eventAction>
          </eventActions>
          <changeFrameCollectionIndexTo>0</changeFrameCollectionIndexTo>
        </event>
      </events>
    </image>
  </images>
</doc>
