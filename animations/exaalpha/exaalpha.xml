<?xml version="1.0"?>
<doc>
  <basic type="animations">
    <screenCanvasResolution>
      <width>100</width>
      <height>100</height>
    </screenCanvasResolution>
    <canvasResolution>
      <maxWidth>100</maxWidth>
      <maxHeight>100</maxHeight>
    </canvasResolution>
    <bgColor>#fffafafa</bgColor>
  </basic>
  <images>
    <image>
      <fileName from="exarotation">cube.png</fileName>
      <imagePropertiesAnimation>
        <propetiesToAnimate>
          <property>
            <propertyName>Alpha</propertyName>
            <minimumAlphaValue>0</minimumAlphaValue>
            <maximumAlphaValue>255</maximumAlphaValue>
            <updateAlphaValue>40</updateAlphaValue>
            <currentAlphaValue>0</currentAlphaValue>
            <animationAlphaAction>Increase</animationAlphaAction>
            <alphaSpeed>1</alphaSpeed>
          </property>
        </propetiesToAnimate>
      </imagePropertiesAnimation>
      <events>
        <event>
          <eventType>MinimumAlphaValueReached</eventType>
          <eventActions>
            <eventAction>ChangeAnimationAlphaAction</eventAction>
          </eventActions>
          <animationAlphaAction>Increase</animationAlphaAction>
        </event>
        <event>
          <eventType>MaximumAlphaValueReached</eventType>
          <eventActions>
            <eventAction>ChangeAnimationAlphaAction</eventAction>
          </eventActions>
          <animationAlphaAction>Decrease</animationAlphaAction>
        </event>
      </events>
      <left>25</left>
      <top>25</top>
      <speed>2</speed>
    </image>
  </images>
</doc>
